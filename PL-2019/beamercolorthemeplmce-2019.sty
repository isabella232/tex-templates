\mode<presentation>

%Custom colors
\definecolor{plmce-2019titleblue}{RGB}{38,47,99}
\definecolor{plmce-2019orange}{RGB}{224,137,69}
\definecolor{plfra-2018gray}{RGB}{171,171,171}
\definecolor{plfra-2018lightgray}{RGB}{215,215,215}

%Element colors
\setbeamercolor*{normal text}{fg=black,bg=white}
\setbeamercolor*{title page header}{fg=plmce-2019titleblue}
\setbeamercolor*{subtitle}{fg=plmce-2019titleblue}
\setbeamercolor*{author}{fg=black}
\setbeamercolor*{date}{fg=black}

\setbeamercolor*{section title}{fg=plmce-2019titleblue,bg=plmce-2019titleblue}
\setbeamercolor*{subsection title}{fg=black,bg=black}

%List markers
\setbeamercolor*{itemize item}{fg=black}
\setbeamercolor*{itemize subitem}{fg=black}
\setbeamercolor*{itemize subsubitem}{fg=black}
%List text
\setbeamercolor{itemize/enumerate body}{fg=black}
\setbeamercolor{itemize/enumerate subbody}{fg=black}
\setbeamercolor{itemize/enumerate subsubbody}{fg=black}

\setbeamercolor*{section in toc}{fg=plmce-2019titleblue}
\setbeamercolor*{subsection in toc}{fg=plmce-2019titleblue}

\setbeamercolor*{frametitle}{fg=plmce-2019titleblue}
\setbeamercolor*{framenumber}{fg=plfra-2018lightgray}

\mode<all>
