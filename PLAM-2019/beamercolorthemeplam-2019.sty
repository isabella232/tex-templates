\mode<presentation>

%Custom colors
\definecolor{plam-2019titleviolet}{RGB}{78,56,141}
\definecolor{plam-2019gray}{RGB}{81,88,130}

\definecolor{plam-2019violet1}{RGB}{61,43,110}
\definecolor{plam-2019violet2}{RGB}{73,51,131}
\definecolor{plam-2019violet3}{RGB}{125,116,162}
\definecolor{plam-2019gray1}{RGB}{181,177,199}

%Element colors
\setbeamercolor*{normal text}{fg=black,bg=white}
\setbeamercolor*{title page header}{fg=plam-2019titleviolet}
\setbeamercolor*{subtitle}{fg=plam-2019titleviolet}
\setbeamercolor*{author}{fg=black}
\setbeamercolor*{date}{fg=black}

\setbeamercolor*{section title}{fg=plam-2019titleviolet,bg=plam-2019titleviolet}
\setbeamercolor*{subsection title}{fg=black,bg=black}

%List markers
\setbeamercolor*{itemize item}{fg=plam-2019violet1}
\setbeamercolor*{itemize subitem}{fg=plam-2019violet2}
\setbeamercolor*{itemize subsubitem}{fg=plam-2019violet3}
%List text
\setbeamercolor{itemize/enumerate body}{fg=plam-2019violet1}
\setbeamercolor{itemize/enumerate subbody}{fg=plam-2019violet2}
\setbeamercolor{itemize/enumerate subsubbody}{fg=plam-2019violet3}

\setbeamercolor*{section in toc}{fg=plmce-2019titleblue}
\setbeamercolor*{subsection in toc}{fg=plmce-2019titleblue}

\setbeamercolor*{frametitle}{fg=plam-2019titleviolet}
\setbeamercolor*{framenumber}{fg=plam-2019gray1}

\mode<all>
